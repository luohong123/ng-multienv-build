{
      "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
      "project": {
            "name": "ng-multienv-build"
      },
      "apps": [
            {
                  "root": "src",
                  "outDir": "dist",
                  "assets": [
                        "assets",
                        "favicon.ico"
                  ],
                  "index": "index.html",
                  "main": "main.ts",
                  "polyfills": "polyfills.ts",
                  "test": "test.ts",
                  "tsconfig": "tsconfig.app.json",
                  "testTsconfig": "tsconfig.spec.json",
                  "prefix": "app",
                  "styles": [
                        "styles.css"
                  ],
                  "scripts": [],
                  "environmentSource": "environments/environment.ts",
                  "environments": {
                        "dev": "environments/environment.ts",
                        "prod": "environments/environment.prod.ts",
                        "dist01": "environments/environment.01.ts",
                        "dist02": "environments/environment.02.ts",
                        "dist03": "environments/environment.03.ts",
                        "dist04": "environments/environment.04.ts",
                        "dist05": "environments/environment.05.ts",
                        "dist06": "environments/environment.06.ts",
                        "dist07": "environments/environment.07.ts",
                        "dist08": "environments/environment.08.ts",
                        "dist09": "environments/environment.09.ts",
                        "dist10": "environments/environment.10.ts",
                        "dist11": "environments/environment.11.ts",
                        "dist12": "environments/environment.12.ts",
                        "dist13": "environments/environment.13.ts",
                        "dist14": "environments/environment.14.ts",
                        "dist15": "environments/environment.15.ts",
                        "dist16": "environments/environment.16.ts",
                        "dist17": "environments/environment.17.ts",
                        "dist18": "environments/environment.18.ts",
                        "dist19": "environments/environment.19.ts",
                        "dist20": "environments/environment.20.ts"
                  }
            }
      ],
      "e2e": {
            "protractor": {
                  "config": "./protractor.conf.js"
            }
      },
      "lint": [
            {
                  "project": "src/tsconfig.app.json",
                  "exclude": "**/node_modules/**"
            },
            {
                  "project": "src/tsconfig.spec.json",
                  "exclude": "**/node_modules/**"
            },
            {
                  "project": "e2e/tsconfig.e2e.json",
                  "exclude": "**/node_modules/**"
            }
      ],
      "test": {
            "karma": {
                  "config": "./karma.conf.js"
            }
      },
      "defaults": {
            "styleExt": "css",
            "component": {}
      }
}